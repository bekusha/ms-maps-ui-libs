{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm",
    "analytics": "92ed70d8-fcff-448b-a900-90ce4928611e"
  },
  "newProjectRoot": "projects",
  "projects": {
    "ms-maps-map-viewer": {
      "projectType": "library",
      "root": "projects/ms-maps-map-viewer",
      "sourceRoot": "projects/ms-maps-map-viewer/src",
      "prefix": "ms",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "tsConfig": "projects/ms-maps-map-viewer/tsconfig.lib.json"
          }
        }
      }
    },
    "testing-libs-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "standalone": false
        },
        "@schematics/angular:directive": {
          "standalone": false
        },
        "@schematics/angular:pipe": {
          "standalone": false
        }
      },
      "root": "projects/testing-libs-app",
      "sourceRoot": "projects/testing-libs-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/testing-libs-app/src/main.ts",
            "tsConfig": "projects/testing-libs-app/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/testing-libs-app/public"
              }
            ],
            "styles": [
              "projects/testing-libs-app/src/styles.css"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "testing-libs-app:build:production"
            },
            "development": {
              "buildTarget": "testing-libs-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/testing-libs-app/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/testing-libs-app/public"
              }
            ],
            "styles": [
              "projects/testing-libs-app/src/styles.css"
            ]
          }
        }
      }
    }
  }
}
